{
	"name": "@deox/gumroad",
	"version": "0.0.3",
	"private": false,
	"description": "API client for Gumroad",
	"keywords": [
		"gumroad",
		"gumroad-api"
	],
	"license": "MIT",
	"author": {
		"name": "Deo Kumar",
		"url": "https://github.com/kumardeo"
	},
	"homepage": "https://github.com/kumardeo/deox/tree/main/packages/gumroad#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/kumardeo/deox",
		"directory": "packages/gumroad"
	},
	"bugs": {
		"url": "https://github.com/kumardeo/deox/issues"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"module": "./dist/esm/index.js",
	"types": "./dist/dts/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/dts/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/dts/index.d.ts"
			]
		}
	},
	"scripts": {
		"lint": "cross-env DEBUG=eslint:cli-engine eslint --fix --ignore-path ../../.eslintignore .",
		"format": "prettier --write --ignore-path ../../.prettierignore .",
		"dev": "wrangler dev",
		"build": "rimraf dist && tsx ./build.ts",
		"prepublishOnly": "tsx ./package.ts",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"console-log-colors": "^0.4.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240419.0",
		"hono": "^4.2.8",
		"wrangler": "^3.52.0"
	}
}
