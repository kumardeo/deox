  "description": "API client for Gumroad",
  "keywords": [
    "gumroad",
    "gumroad-api"
  ],
  "license": "MIT",
  "author": {
    "name": "Deo Kumar",
    "url": "https://github.com/kumardeo"
  },
  "homepage": "https://github.com/kumardeo/deox/tree/main/packages/gumroad#readme",
