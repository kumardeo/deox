			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"dependencies": {
		"@deox/utils": "workspace:^",
		"console-log-colors": "^0.5.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240925.0",
		"hono": "^4.6.3",
		"wrangler": "^3.78.12"
	}
}
